{
  "name": "level-atomics",
  "version": "2.1.0",
  "description": "Atomic and multi friendly operations for LevelDB",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha",
    "test-cov": "istanbul cover --dir test/coverage _mocha && echo open test/coverage/lcov-report/index.html",
    "test-reload": "nodemon --ignore test/coverage -w ./ -x \"npm test\"",
    "test-reload-cov": "nodemon --ignore test/coverage -w ./ -x \"npm run test-cov\"",
    "test-travis": "istanbul cover node_modules/.bin/_mocha --report lcovonly"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IndigoUnited/node-level-atomics.git"
  },
  "author": "Marco Oliveira <marcooliveira@me.com>",
  "keywords": [
    "level",
    "leveldb",
    "level-db",
    "increment",
    "decrement",
    "counter",
    "count",
    "lock",
    "replace",
    "insert",
    "multi",
    "multiple",
    "atomic",
    "locking"
  ],
  "license": "MIT",
  "homepage": "https://github.com/IndigoUnited/node-level-atomics",
  "devDependencies": {
    "codacy-coverage": "^1.1.2",
    "coveralls": "^2.11.2",
    "expect.js": "^0.3.1",
    "istanbul": "^0.3.17",
    "leveldown": "^1.3.0",
    "levelup": "^1.2.1",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "0.0.2"
  },
  "dependencies": {
    "async": "^1.3.0",
    "lock": "^0.1.2"
  }
}
